@{
    ViewData["Title"] = "Delete";
}
<div class="text-center">
    <h2 class="display-4">Удаление склада</h2>
</div>
<form method="post">
    <div class="row">
        <div class="col-4">Склад:</div>
        <div class="col-8">
            <select id="warehouse" name="warehouse" class="form-control" asp-items="@(new SelectList(@ViewBag.Warehouses,"Id", "WarehouseName"))"></select>
        </div>
    </div>
    <div class="row">
        <div class="col-8"></div>
        <div class="col-4"><input type="submit" value="Удалить" class="btn btnprimary" /></div>
    </div>
</form> 